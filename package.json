{
  "name": "js-schema-tools",
  "version": "1.0.6",
  "description": "A collection of schema-enabled JS tools",
  "main": "index.js",
  "type": "module",
  "bin": {
    "jss-makemigration": "./lib/schema/migrations/cli.js"
  },
  "scripts": {
    "test": "npm run test:jest:models && npm run test:jest:migrations",
    "test:jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose=false",
    "test:jest:models": "npm run test:jest -- test/model.test.js",
    "test:jest:migrations": "npm run test:jest -- test/migration.test.js"
  },
  "keywords": [
    "js",
    "model",
    "schema",
    "diff",
    "equals"
  ],
  "author": "Pomax",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.2.2"
  }
}
